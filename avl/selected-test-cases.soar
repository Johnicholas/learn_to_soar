setup {

source avl.soar

sp {initialize
    (state <s> ^superstate nil)
-->
    (<s> ^name test-avl)}

source test-comparisons.soar

sp {pass*nothing
    (state <s> ^name test-avl ^answer-should-be nothing ^answer-is nothing)
-->
    (pass)}

sp {pass*not*nothing
    (state <s> ^name test-avl ^answer-should-be <n> ^answer-is.just.test <n>)
-->
    (pass)}
}

